{
  "name": "next-hono-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run dev:next & npm run dev:hcm & npm run dev:mastra",
    "dev:next": "next dev",
    "dev:mastra": "mastra dev --dir mastra",
    "build:mastra": "mastra build --dir mastra",
    "dev:hcm": "hcm 'src/**/*.module.css' -w",
    "hcm": "hcm 'src/**/*.module.css'",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:js": "eslint --ext .ts,.tsx,.js --ignore-path .gitignore .",
    "lint:prettier": "prettier --check \"./**/*.{ts,tsx,js}\" --ignore-path .gitignore",
    "lint:style": "stylelint \"src/**/*.css\" --ignore-path .gitignore",
    "lint:fix": "npm run lint:js -- --fix && prettier --write \"./**/*.{ts,tsx,js}\" --ignore-path .gitignore && npm run lint:style -- --fix",
    "typecheck": "npm run hcm && tsc --noEmit",
    "format": "prettier --write \"./**/*.{ts,tsx,js,json,css,md}\" --ignore-path .gitignore"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.3.22",
    "@geolonia/open-reverse-geocoder": "^0.1.0",
    "@mantine/core": "^7.17.0",
    "@mantine/form": "^8.0.2",
    "@mantine/hooks": "^7.17.0",
    "@mantinex/dev-icons": "^2.0.0",
    "@mantinex/mantine-logo": "^2.0.0",
    "@mastra/core": "^0.10.1",
    "@mastra/libsql": "^0.10.0",
    "@mastra/loggers": "^0.10.0",
    "@mastra/memory": "^0.10.1",
    "@next/eslint-plugin-next": "^15.2.1",
    "@supabase/auth-helpers-react": "^0.5.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.50.0",
    "@tabler/icons-react": "^3.33.0",
    "cheerio": "^1.0.0",
    "framer-motion": "^12.16.0",
    "geist": "^1.4.2",
    "hono": "^4.7.0",
    "lucide-react": "^0.511.0",
    "mastra": "^0.10.1",
    "next": "^14.2.23",
    "nominatim-browser": "^2.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript-eslint": "^8.26.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@prisma/client": "^6.5.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "eslint": "^9.22.0",
    "eslint-config-flat-gitignore": "^2.1.0",
    "eslint-config-next": "15.1.4",
    "eslint-config-prettier": "^10.1.1",
    "happy-css-modules": "^3.2.0",
    "i": "^0.3.7",
    "jiti": "^2.4.2",
    "npm": "^11.2.0",
    "prettier": "3.5.3",
    "prisma": "^6.9.0",
    "stylelint": "^15.11.0",
    "stylelint-config-standard": "^31.0.0",
    "typescript": "^5.8.2"
  }
}